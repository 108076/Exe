#include <stdio.h>

int main() {
    int a, b, product;
    a = 100;
    b = 1333;

 
    // Calculating product
    product = a * b;

 
    printf("Product = %d", product);
    
    product = a +b;

 
    printf("Product = %d", product);
    
    return 0;
    
    product = a -b;

 
    printf("Product = %d", product);
    
    return 0;
    
    printf("Product = %d", product);
    
    return 0;
    
    product = a /b;
    
    printf("Product = %d", product);
    
    return 0;
    
    product = a %b;
}
